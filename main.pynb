    "import cv2\n",
    "import numpy as np\n",
    "from tensorflow.keras.models import load_model\n",
    "\n",
    "# 1️⃣ Load Haar cascade and trained model\n",
    "face_cascade = cv2.CascadeClassifier(\"haarcascade_frontalface_default.xml\")\n",
    "model = load_model(\"face_recognition_model.h5\")\n",
    "\n",
    "# 2️⃣ Start webcam\n",
    "cap = cv2.VideoCapture(0)\n",
    "\n",
    "while True:\n",
    "    ret, frame = cap.read()\n",
    "    if not ret:\n",
    "        break\n",
    "\n",
    "    gray = cv2.cvtColor(frame, cv2.COLOR_BGR2GRAY)\n",
    "    faces = face_cascade.detectMultiScale(gray, 1.3, 5)\n",
    "\n",
    "    for (x, y, w, h) in faces:\n",
    "        face = gray[y:y+h, x:x+w]\n",
    "        face = cv2.resize(face, (50,50))\n",
    "        face_input = face.reshape(1,50,50,1) / 255.0\n",
    "\n",
    "        # 3️⃣ Predict using the trained model\n",
    "        prediction = model.predict(face_input)[0][0]\n",
    "\n",
    "        if prediction > 0.5:\n",
    "            label = \"Shalu - You can enter\"\n",
    "            color = (0,255,0)  # green\n",
    "        else:\n",
    "            label = \"Unknown - Access Denied\"\n",
    "            color = (0,0,255)  # red\n",
    "\n",
    "        # Draw rectangle and label\n",
    "        cv2.rectangle(frame, (x,y), (x+w, y+h), color, 2)\n",
    "        cv2.putText(frame, label, (x, y-10),\n",
    "                    cv2.FONT_HERSHEY_SIMPLEX, 0.8, color, 2)\n",
    "\n",
    "    cv2.imshow(\"Smart Door Access\", frame)\n",
    "\n",
    "    # Press Enter to exit\n",
    "    if cv2.waitKey(1) == 13:\n",
    "        break\n",
    "\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()\n"
   ]
